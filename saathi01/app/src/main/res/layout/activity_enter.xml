<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Enter">

    <TextView
        android:id="@+id/aquaboy"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#262627"
        android:gravity="center"
        android:padding="20dp"
        android:text="Saathi"
        android:textColor="#F0E8E8"
        android:textSize="40dp" />

    <TextView
        android:id="@+id/tv5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/aquaboy"
        android:layout_marginTop="0dp"
        android:background="#FA0F33" />

    <Button
        android:id="@+id/ayu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv5"

        android:onClick="au"
        android:background="@drawable/bg"
        android:padding="30dp"
        android:text="AYURVEDIC CARE"
        android:textSize="15dp" />

    <Button
        android:id="@+id/car"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ayu"

        android:onClick="ca"

        android:background="@drawable/bg"
        android:padding="30dp"

        android:text="CARDIAC CARE"
        android:textSize="15dp" />

    <Button
        android:id="@+id/dia"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/car"

        android:onClick="di"
        android:background="@drawable/bg"
        android:padding="30dp"
        android:text="DIABETIC CARE"
        android:textSize="15dp" />

    <Button
        android:id="@+id/hyp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dia"
        android:layout_alignParentStart="true"
        android:background="@drawable/bg"
        android:onClick="hy"
        android:padding="30dp"
        android:text="HYPERTENSION"
        android:textSize="15dp" />

    <Button
        android:id="@+id/liv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/hyp"
        android:background="@drawable/bg"
        android:padding="30dp"
        android:onClick="li"
        android:text="LIVER CARE"
        android:textSize="15dp" />

    <Button

        android:id="@+id/child"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/liv"

        android:onClick="ch"

        android:background="@drawable/bg"
        android:padding="30dp"
        android:text="CHILD CARE"
        android:textSize="15dp" />





    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="692dp"
        android:background="#103342" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="711dp"
        android:background="#000" />


</RelativeLayout>